    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  }
}
