{
  "name": "rentx",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect=0.0.0.0:9229 --transpile-only --ignore-watch node_modules src/shared/infra/http/server/index.ts",
    "test": "NODE_ENV=test jest --runInBand",
    "test:verbose": "NODE_ENV=test jest --runInBand --verbose",
    "test:watchAll": "NODE_ENV=test jest --runInBand --watchAll",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "migration:create": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop",
    "seed:run": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "generate": "plop --plopfile generators/plopfile.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@jest/types": "^26.6.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.5",
    "@types/nodemailer": "^6.4.1",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@sentry/node": "^6.3.5",
    "@sentry/tracing": "^6.3.5",
    "aws-sdk": "^2.886.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "csv-parse": "^4.15.3",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "envalid": "^7.1.0",
    "express": "^4.17.1",
